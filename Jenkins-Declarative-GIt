pipeline {
    agent any 
    environment {
     PATH = "/tmp/apache-maven-3.9.5/bin:$PATH"
    }
    stages{
        stage ("Git CHeckout") {
            steps {
                     git credentialsId: 'github', url: 'https://github.com/sunil566/myweb'
            }
            }
           stage(""Maven Build") {
              steps{
                sh "mvn clean package"
              }
           }
        }
    }
